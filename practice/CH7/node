class Node:
    # 변수 포인터, 값
    def __init__(self, data):
        self.__data= data
        self.__next = None

    @property
    def data(self):
        return self.__data

    @data.setter
    def data(self, data):
        self.__data = data

    @property
    def next(self):
        return self.__next

    @next.setter
    def next(self, newnext):
        self.__next = newnext
